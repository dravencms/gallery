{
  "name": "dravencms/gallery",
  "type": "dravencms-package",
  "description": "Gallery module for dravencms",
  "keywords": [
    "dravencms",
    "gallery"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "dravencms/admin": ">=1.0",
    "dravencms/tag": ">=1.0",
    "dravencms/structure": ">=1.0"
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "gallery": "Dravencms\\Gallery\\DI\\GalleryExtension"
        },
        "doctrine":{
          "metadata": {
            "Dravencms\\Model\\Gallery": "%appDir%/../vendor/dravencms/gallery/src/Dravencms/Model/Gallery"
          }
        }
      },
      "scripts": {
        "post-install": "Dravencms\\Gallery\\Script\\PostInstall",
        "pre-uninstall": "Dravencms\\Gallery\\Script\\PreUninstall"
      }
    }
  }
}
